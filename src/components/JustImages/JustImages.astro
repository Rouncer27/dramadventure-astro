---
import { Image } from "astro:assets";
import "./justImages.scss";
const { data } = Astro.props;

console.log("data", data);
---

<div class="just-images">
  <div class="just-images-wrapper">
    <div class="just-images-one">
      <Image
        src={data.imageOne.sourceUrl}
        alt={data.imageOne.altText}
        width={1500}
        height={1500}
      />
    </div>
    <div class="just-images-one">
      <Image
        src={data.imageTwo.sourceUrl}
        alt={data.imageTwo.altText}
        width={1500}
        height={1500}
      />
    </div>
  </div>
</div>
